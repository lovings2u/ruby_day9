<%= form_tag("/boards") do %>
    <%= text_field_tag(:title, nil, class: "form-control text-center", placeholder: "제목을 입력하세요") %>
    <%= text_area_tag(:contents, nil, class: "form-control text-center", placeholder: "내용을 입력하세요", rows: 6) %>
    <%= submit_tag("작성하기", class: "btn btn-primary mt-10") %>
<% end %>